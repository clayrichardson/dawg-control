{
	"version": 1,
	"allow_edit": true,
	"plugins": [],
	"panes": [
		{
			"width": 1,
			"row": {
				"3": 1
			},
			"col": {
				"3": 1
			},
			"col_width": 1,
			"widgets": [
				{
					"type": "sparkline",
					"settings": {
						"title": "RandInt",
						"value": [
							"datasources[\"postgrest\"][0][\"value\"]"
						],
						"include_legend": true,
						"legend": "shits to give"
					}
				}
			]
		}
	],
	"datasources": [
		{
			"name": "postgrest",
			"type": "JSON",
			"settings": {
				"url": "http://noname.local:3000/data?order=time.desc&name=eq.mock.randint",
				"use_thingproxy": false,
				"refresh": 1,
				"method": "GET",
				"headers": [
					{
						"name": "Range",
						"value": "1-1"
					}
				]
			}
		}
	],
	"columns": 3
}